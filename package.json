{
  "name": "@flexdevelopment/dbktv",
  "description": "DiamondbackTV (DBKTV) is a digital signage project with the goal of streaming specially curated Diamondback content - feature stories, multimedia, article previews, and advertisements - to various televisions on campus.",
  "version": "1.0.0",
  "author": "Lexus Drumgold <lex@lexusdrumgold.design> (https://lexusdrumgold.design)",
  "scripts": {
    "preinstall": "run-script-os",
    "preinstall:win32": "(IF EXIST node_modules\\ (RMDIR /Q /S node_modules > NUL)) && npm run prebuild",
    "preinstall:nix": "rm -rf node_modules && npm run prebuild",
    "install:default": "",
    "lint": "eslint ./src/api/*.js ./src/components/*.jsx ./src/components/**/*.jsx --fix",
    "lint-watch": "esw -w ./src/api/*.js ./src/components/*.jsx ./src/components/**/*.js ./src/components/**/*.jsx --fix",
    "watch": "run-script-os",
    "watch:win32": "cross-env NODE_ENV=development COPY .env.development .env && concurrently \"npm:lint-watch\" \"react-scripts start\"",
    "watch:nix": "cross-env NODE_ENV=development cp .env.development .env && concurrently \"npm:lint-watch\" \"react-scripts start\"",
    "postwatch": "run-script-os",
    "postwatch:win32": "DEL /Q .env > NUL",
    "postwatch:nix": "rm -rf .env",
    "dev": "run-script-os",
    "dev:win32": "cross-env NODE_ENV=development COPY .env.development .env && react-scripts start",
    "dev:nix": "cross-env NODE_ENV=development cp .env.development .env && react-scripts start",
    "postdev": "run-script-os",
    "postdev:win32": "DEL /Q .env > NUL",
    "postdev:nix": "rm -rf .env",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --passWithNoTests",
    "prebuild": "run-script-os",
    "prebuild:win32": "IF EXIST build\\ ( RMDIR /Q /S build > NUL)",
    "prebuild:nix": "rm -rf build/",
    "build": "run-script-os",
    "build:default": "react-scripts build",
    "start": "run-script-os",
    "start:win32": "cross-env NODE_ENV=staging COPY .env.production .env && npm run lint && npm run build && firebase serve --only hosting:staging",
    "start:nix": "cross-env NODE_ENV=staging cp .env.production .env && npm run lint && npm run build && firebase serve --only hosting:staging",
    "poststart": "run-script-os",
    "poststart:win32": "DEL /Q .env > NUL",
    "poststart:nix": "rm -rf .env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-Diamondback-Lab/dbktv.git"
  },
  "keywords": [
    "the diamondback",
    "digital signage",
    "iadea player",
    "react",
    "sass",
    "web application"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/The-Diamondback-Lab/dbktv/issues"
  },
  "dependencies": {
    "@feathersjs/errors": "^3.3.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "concurrently": "^4.1.2",
    "firebase": "^6.4.1",
    "jquery": "^3.4.1",
    "react": "^16.9.0",
    "react-dfp": "^0.13.0",
    "react-dom": "^16.9.0",
    "react-ga": "^2.6.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "showdown": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-jest": "^24.9.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.2.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^7.0.0",
    "eslint-config-standard-react": "^9.0.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^6.0.0",
    "firebase-tools": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.12.0",
    "react-axe": "^3.2.0",
    "run-script-os": "^1.0.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "10"
  }
}