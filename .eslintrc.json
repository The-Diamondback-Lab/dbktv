{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true,
    "jquery": true
  },
  "extends": [
    "plugin:jest/recommended",
    "plugin:react/recommended",
    "standard",
    "standard-preact",
    "standard-jsx"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    },
    "ecmaVersion": 6,
    "requireConfigFile": false,
    "sourceType": "module"
  },
  "root": true,
  "rules": {
    "camelcase": 0,
    "react/jsx-key": 0,
    "react/prop-types": 0,
    "space-before-function-paren": 0
  }
}